{
  "parser": "babel-eslint",
  "extends": "eslint-config-google",
  "env":{
    "node": false,
    "browser": true,
    "es6": true
  },
  "rules": {
    "arrow-body-style": [0],
    "comma-dangle":[2, "never"],
    "global-require": [0],
    "max-len":[0],
    "no-console": [2, { "allow": ["log", "warn", "error"] }],
    "no-underscore-dangle": [2, { "allow": ["_GET", "_POST", "_id", "_ERROR"] }],
    "no-unused-expressions": [0, { "allowTernary": true }],
    "no-unused-vars": [2, { "varsIgnorePattern": "React" }],
    "no-nested-ternary": [0],
    "no-param-reassign": [0],
    "no-plusplus": [0],
    "newline-per-chained-call": [0],
    "semi": [2, "always"]
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  }
}
